generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id                   String         @id @default(cuid())
  firstName            String
  lastName             String?
  image                String?
  email                String         @unique
  username             String         @unique
  password             String?
}
